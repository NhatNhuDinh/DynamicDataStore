<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view"
      id="DynamicDataStore"
      title="msg://dynamicDataStore.title">

    <data>
        <keyValueCollection id="rowsDc">
            <properties>
                <property name="name"   datatype="string"/>
            </properties>
        </keyValueCollection>
    </data>

    <layout>
        <vbox spacing="true" padding="true" width="100%" height="100%">
            <dataGrid id="rowsGrid"
                      dataContainer="rowsDc"
                      width="100%"
                      height="100%">
            </dataGrid>
        </vbox>
    </layout>
</view>
